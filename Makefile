default:
	@echo "This is merely a makefile stub to ensure that you use GNU make instead".

test:
	gmake test

home:
	gmake home

nethack-home:
	gmake nethack-home

slashem-home:
	gmake slashem-home

distfiles:
	gmake distfiles
